<!DOCTYPE html>

<html lang="fa">
<head>
    <meta name="viewport" content="width=device-width" />
    <title> پرشین درب |  @ViewData["Title"]</title>

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Place favicon.ico in the root directory -->
    <link href="~/shared/main/assets/images/favicon.ico" type="img/x-icon" rel="shortcut icon">
    <!-- All css files are included here. -->
    <link rel="stylesheet" href="~/shared/main/assets/css/vendor/bootstrap.min.css">
    <link rel="stylesheet" href="~/shared/main/assets/css/vendor/iconfont.min.css">
    <link rel="stylesheet" href="~/shared/main/assets/css/vendor/helper.css">
    <link rel="stylesheet" href="~/shared/main/assets/css/plugins/plugins.css">
    <link rel="stylesheet" href="~/shared/main/assets/css/style.css">
    <link href="~/shared/main/assets/css/customize.css" rel="stylesheet" />
    <!-- Modernizr JS -->
    <script src="~/shared/main/assets/js/vendor/modernizr-2.8.3.min.js"></script>

</head>
<body>
    <div id="main-wrapper">
        @* --------- Header & Menu --------- *@
        @await Html.PartialAsync("~/Views/Shared/Header/_PartialHeader.cshtml")

        @* =================== Start Main ======================  *@
        <div>
            @RenderBody()
        </div>
        @* ==================== End Main =======================  *@

        @* ------------- Footer ------------ *@
        @await Html.PartialAsync("~/Views/Shared/Footer/_PartialFooter.cshtml")
    </div>

    <!-- =========================
        Main Loding JS Script
    ============================== -->
    <!-- All jquery file included here -->
    <script src="~/shared/main/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="https://maps.google.com/maps/api/js?sensor=false&amp;libraries=geometry&amp;v=3.22&amp;key=AIzaSyDAq7MrCR1A2qIShmjbtLHSKjcEIEBEEwM"></script>
    <script src="~/shared/main/assets/js/vendor/popper.min.js"></script>
    <script src="~/shared/main/assets/js/vendor/bootstrap.min.js"></script>
    <script src="~/shared/main/assets/js/plugins/plugins.js"></script>
    <script src="~/shared/main/assets/js/main.js"></script>

</body>
</html>
